# 計算器大集合

print("考試分數計算器")
a=int(input("輸入考試分數"))
if a>95 :
    print("天才")
elif a>90 :
    print("很棒")
elif a>80 :
    print("不錯")
elif a<30 :
    print("糟糕")
elif a<60 :
    print("加油")
else :
    print("合格")



print("園面積計算器")
a=int(input("輸入直徑"))
e=a//2
f=e*e*3.1415926
print(e,"×",e,"×",3.1415926,"=",f,sep="")



print("BMI計算器")
a=int(input("輸入體重"))
r=int(input("輸入身高"))
o=r//a
if o<4:
    print("胖")
elif o>8:
    print("瘦")
else :
    print("正常")
